{
	"name": "plex-ripper-web-ui",
	"version": "0.5.0",
	"description": "PlexRipper",
	"author": "JasonLandbridge",
	"private": true,
	"scripts": {
		"dev": "nuxt-ts dev --no-inline",
		"build": "nuxt-ts build",
		"generate": "nuxt-ts generate",
		"generate-ts": "npx swagger-typescript-api -p http://localhost:5000/swagger/v1/swagger.json --no-client -o ./src/types/dto/ -n mainApi.ts",
		"start": "nuxt-ts start",
		"lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
		"lint-fix": "eslint . --ext .ts,.js,.vue --max-warnings=0 --fix & prettier-stylelint --ignore-path ./.eslintignore --write \"{,!(node_modules)/**/}*.{css,scss}\" --fix & prettier --ignore-path ./.eslintignore --write \"{,!(node_modules)/**/}*.{ts,vue,json,js,jsx}\"",
		"test": "jest",
		"update-web-types": "vue-docgen-web-types",
		"npm-upgrade-packages": "npm-upgrade"
	},
	"lint-staged": {
		"*.{js,vue}": "npm run lint --fix",
		"*.{css,vue}": "stylelint"
	},
	"husky": {
		"hooks": {
			"pre-commit": "npm run lint"
		}
	},
	"web-types": "./web-types.json",
	"dependencies": {
		"@aspnet/signalr": "^1.1.4",
		"@codewithdan/observable-store": "^2.2.11",
		"@mdi/font": "^5.8.55",
		"@nuxt/types": "^2.14.12",
		"@nuxt/typescript-runtime": "^2.0.0",
		"@nuxtjs/axios": "^5.12.4",
		"@ssv/signalr-client": "^2.0.0",
		"@types/node": "^14.14.14",
		"axios-observable": "^1.1.3",
		"consola": "^2.15.0",
		"date-fns": "^2.16.1",
		"fluent-type-results": "^1.0.11",
		"lodash": "^4.17.20",
		"material-design-icons-iconfont": "latest",
		"nuxt": "^2.14.12",
		"nuxt-i18n": "^6.16.0",
		"object-path": "^0.11.5",
		"rxjs": "^6.6.3",
		"swagger-typescript-api": "^3.0.1",
		"three": "^0.125.0",
		"vanta": "^0.5.21",
		"vue": "^2.6.12",
		"vue-class-component": "^7.2.6",
		"vue-docgen-web-types": "^0.1.7",
		"vue-property-decorator": "^9.1.2",
		"vue-status-indicator": "^1.2.1",
		"vue2-perfect-scrollbar": "^1.5.0"
	},
	"devDependencies": {
		"@nuxt/typescript-build": "^2.0.3",
		"@nuxtjs/eslint-config-typescript": "^5.0.0",
		"@nuxtjs/eslint-module": "^3.0.2",
		"@nuxtjs/stylelint-module": "^4.0.0",
		"@nuxtjs/vuetify": "^1.11.2",
		"@types/lodash": "^4.14.165",
		"@vue/test-utils": "^1.1.2",
		"babel-eslint": "^10.1.0",
		"babel-jest": "^26.6.3",
		"eslint": "^7.15.0",
		"eslint-config-prettier": "^7.0.0",
		"eslint-plugin-nuxt": "^2.0.0",
		"eslint-plugin-prettier": "^3.3.0",
		"husky": "^4.3.6",
		"jest": "^26.6.3",
		"lint-staged": "^10.5.3",
		"prettier": "^2.2.1",
		"stylelint": "^13.8.0",
		"ts-jest": "^26.4.4",
		"vue-jest": "^4.0.0-rc.0",
		"vuex-module-decorators": "^1.0.1"
	}
}
