{
  "name": "plex-ripper-web-ui",
  "version": "0.9.1",
  "description": "PlexRipper",
  "author": "JasonLandbridge",
  "private": true,
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "generate": "nuxt generate --fail-on-error",
    "generate-ts": "npx swagger-typescript-api -p http://localhost:5000/swagger/v1/swagger.json --no-client -o ./src/types/dto/ -n mainApi.ts",
    "start": "nuxt start",
    "start-static": "nuxt generate --fail-on-error & nuxt start",
    "lint-fix": "eslint . --ext .ts,.js,.vue --max-warnings=0 --fix & prettier-stylelint --ignore-path ./.eslintignore --write \"{,!(node_modules)/**/}*.{css,scss}\" --fix & prettier --ignore-path ./.eslintignore --write \"{,!(node_modules)/**/}*.{ts,vue,json,js,jsx}\"",
    "lint-staged": "lint-staged",
    "npm-upgrade-packages": "npm-upgrade",
    "analyze": "nuxt build --analyze",
    "cypress:e2e": "start-server-and-test test-dev http://localhost:3030 cypress:open",
    "cypress:e2e:silent": "start-server-and-test start http://localhost:3030 cypress:run",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "pree2e:slient": "npm run build",
    "test-dev": "nuxt dev --port 3030",
    "test": "jest"
  },
  "script-comments": {
    "generate-ts": "Run this to generate typescript definitions based on the back-end Swagger API documentation, can only be run if back-end is running",
    "lint-staged": "Automatically run by Husky to check for lint errors when commiting changes",
    "npm-upgrade-packages": "This can be run to help upgrade packages to newer versions and track why some are not updated."
  },
  "lint-staged": {
    "*.{js,vue}": "npm run lint --fix",
    "*.{css,vue}": "stylelint"
  },
  "dependencies": {
    "@codewithdan/observable-store": "^2.2.15",
    "@faker-js/faker": "^7.6.0",
    "@mdi/font": "^7.0.96",
    "@microsoft/signalr": "^6.0.8",
    "@nuxt/typescript-runtime": "^2.1.0",
    "@nuxtjs/i18n": "^7.3.0",
    "@vueuse/rxjs": "^9.1.1",
    "axios": "^1.2.0",
    "axios-observable": "^2.0.0",
    "consola": "^2.15.3",
    "date-fns": "^2.29.3",
    "jsdom-worker": "^0.3.0",
    "lodash-es": "^4.17.21",
    "material-design-icons-iconfont": "^6.7.0",
    "nuxt": "^2.15.8",
    "object-path": "^0.11.8",
    "rxjs": "^7.5.7",
    "three": "^0.123.0",
    "vanta": "^0.5.22",
    "vue": "^2.6.14",
    "vue-class-component": "^7.2.6",
    "vue-property-decorator": "^9.1.2",
    "vue-status-indicator": "^1.2.1",
    "vue-verification-code-input": "^1.0.0",
    "vuescroll": "^4.17.5",
    "vuetify": "^2.6.12"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.18.6",
    "@hirez_io/observer-spy": "^2.2.0",
    "@intlify/eslint-plugin-vue-i18n": "^2.0.0",
    "@nuxt/components": "^2.2.1",
    "@nuxt/postcss8": "^1.1.3",
    "@nuxt/types": "^2.15.8",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/eslint-config-typescript": "^11.0.0",
    "@nuxtjs/eslint-module": "^3.1.0",
    "@nuxtjs/stylelint-module": "^4.1.0",
    "@nuxtjs/vuetify": "^1.12.3",
    "@types/jest": "^26.0.23",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^16.18.1",
    "@vue/test-utils": "^1.3.3",
    "@vueuse/core": "^9.1.1",
    "@vueuse/nuxt": "^9.1.1",
    "axios-mock-adapter": "^1.21.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "cypress": "^12.0.1",
    "cypress-vue-unit-test": "^3.5.1",
    "cypress-wait-until": "^1.7.2",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.4.0",
    "husky": "^8.0.1",
    "jest": "^26.6.3",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "start-server-and-test": "^1.14.0",
    "stylelint": "^14.11.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^28.0.0",
    "swagger-typescript-api": "^12.0.2",
    "ts-jest": "^26.5.6",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "vue-jest": "^3.0.4"
  }
}
