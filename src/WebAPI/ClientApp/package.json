{
	"name": "plex-ripper-web-ui",
	"version": "0.8.7",
	"description": "PlexRipper",
	"author": "JasonLandbridge",
	"private": true,
	"scripts": {
		"dev": "nuxt dev",
		"build": "nuxt build",
		"generate": "nuxt generate --fail-on-error",
		"generate-ts": "npx swagger-typescript-api -p http://localhost:5000/swagger/v1/swagger.json --no-client -o ./src/types/dto/ -n mainApi.ts",
		"start": "nuxt start",
		"start-static": "nuxt generate --fail-on-error & nuxt start",
		"lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
		"lint-fix": "eslint . --ext .ts,.js,.vue --max-warnings=0 --fix & prettier-stylelint --ignore-path ./.eslintignore --write \"{,!(node_modules)/**/}*.{css,scss}\" --fix & prettier --ignore-path ./.eslintignore --write \"{,!(node_modules)/**/}*.{ts,vue,json,js,jsx}\"",
		"test": "jest",
		"update-web-types": "vue-docgen-web-types -c web-types-config.json",
		"npm-upgrade-packages": "npm-upgrade",
		"analyze": "nuxt build --analyze"
	},
	"lint-staged": {
		"*.{js,vue}": "npm run lint --fix",
		"*.{css,vue}": "stylelint"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"web-types": "./web-types.json",
	"dependencies": {
		"@codewithdan/observable-store": "^2.2.13",
		"@mdi/font": "^5.9.55",
		"@microsoft/signalr": "^5.0.10",
		"@nuxt/typescript-runtime": "^2.1.0",
		"@nuxtjs/i18n": "^7.0.3",
		"@types/node": "^16.6.1",
		"axios-observable": "^1.2.1",
		"consola": "^2.15.3",
		"date-fns": "^2.23.0",
		"lodash": "^4.17.21",
		"material-design-icons-iconfont": "latest",
		"npm": "^8.11.0",
		"nuxt": "^2.15.8",
		"object-path": "^0.11.5",
		"rxjs": "^7.2.0",
		"swagger-typescript-api": "^9.2.0",
		"three": "^0.123.0",
		"ts-simple-nameof": "^1.3.1",
		"typescript-collections": "^1.3.3",
		"vanta": "^0.5.21",
		"vue": "^2.6.14",
		"vue-class-component": "^7.2.6",
		"vue-docgen-web-types": "^0.1.7",
		"vue-property-decorator": "^9.1.2",
		"vue-rx": "^6.2.0",
		"vue-status-indicator": "^1.2.1",
		"vue-verification-code-input": "^1.0.0",
		"vuescroll": "^4.17.3",
		"vuetify": "^2.5.8"
	},
	"devDependencies": {
		"@intlify/eslint-plugin-vue-i18n": "^0.15.0",
		"@nuxt/components": "^2.2.1",
		"@nuxt/types": "^2.15.8",
		"@nuxt/typescript-build": "^2.1.0",
		"@nuxtjs/eslint-config-typescript": "^6.0.1",
		"@nuxtjs/eslint-module": "^3.0.2",
		"@nuxtjs/stylelint-module": "^4.0.0",
		"@nuxtjs/vuetify": "^1.12.1",
		"@types/jest": "^26.0.24",
		"@types/lodash": "^4.14.172",
		"@vue/test-utils": "^1.2.2",
		"babel-eslint": "^10.1.0",
		"babel-jest": "^27.0.6",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-nuxt": "^2.0.0",
		"eslint-plugin-prettier": "^3.4.0",
		"husky": "^4.3.8",
		"jest": "^27.0.6",
		"lint-staged": "^11.1.2",
		"prettier": "^2.3.2",
		"stylelint": "^13.13.1",
		"ts-jest": "^27.0.4",
		"tsconfig-paths-webpack-plugin": "^3.5.1",
		"vue-jest": "^4.0.1"
	}
}
